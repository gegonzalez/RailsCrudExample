<%= form_for @products do |f| %>
  <div class="form-group">
    <%= f.label :sku %>
    <%= f.text_field :sku, class: "form-group" %>
  </div>
    <div class="form-group">
      <%= f.label :nombre %>
      <%= f.text_field :nombre, class: "form-group" %>
    </div>
    <div class="form-group">
      <%= f.label :precio %>
      <%= f.text_field :precio, class: "form-group" %>
    </div>
    <%= f.submit class: "btn btn-primary" %>
<% end %>